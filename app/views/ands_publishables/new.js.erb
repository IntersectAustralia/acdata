<% if moderators.present? %>
$('#new_ands_publishable_dialog').jqmHide();
$('#wizard').html('<%= escape_javascript(render :partial => 'form', :locals => {:action => "new"}) %>');
$('#new_ands_publishable_dialog').jqm({modal:true});
$('#new_ands_publishable_dialog').jqmShow();
$('[id^=show_][id$=_wizard]').removeClass('ac_loading');
$('#share_dropdown').removeClass('ac_loading');
<% else %>
<% flash[:alert] = "You cannot publish data if there are no moderators." %>
window.location.reload();
<% end %>
